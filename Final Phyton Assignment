   "source": [
    "df_after_merger[\"Q\"] = df_after_merger['output_non_merging_firm'] + df_after_merger['output_merged_firm'] #Q is formed by adding up the outputs of firm 1 and the merged firms.\n",
    "df_after_merger[\"P\"] = 1 - df_after_merger[\"Q\"]                                                           #P is formed by substracting Q from 1 as described at the start of this test.\n",
    "df_after_merger[\"H\"] = (df_after_merger['output_non_merging_firm']/df_after_merger[\"Q\"])**2 + (df_after_merger['output_merged_firm']/df_after_merger[\"Q\"])**2 #H is formed bij adding up the Herfindahl Index of firm 1 to the Herfindahl Index of the merged firms. \n",
    "\n",
    "df_after_merger.head()          #Shows us the head of our dataframe."
   ]
  },
